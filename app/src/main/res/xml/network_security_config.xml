<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- 配置 debug 模式下，信任用户证书   -->
    <debug-overrides>
        <trust-anchors>
            <certificates src="system"/>
            <certificates src="user"/>
        </trust-anchors>
    </debug-overrides>
</network-security-config>